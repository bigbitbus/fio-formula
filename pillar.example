fio:
  lookup:
    package: 'fio'
    install_from_source: True
    stressng_source_url:  "https://github.com/axboe/fio/archive/master.tar.gz"

execute:
  lookup:
    out_dir: '/tmp/outputdata/diskchecker'
    disk_file: '/disk/file' #e.g. /dev/sdb for sdb
    job_file_list: 
      - testing.job
      